<?xml version="1.0" encoding="utf-8"?>
<MsvcConf>
  <Project Name="libDB">
    <Include Name="sqlite3.c" Define="HAVE_SQLITE3_AMALGAMATION" />
  </Project>
  
  <Project Name="OOServer">
    <Include Name="sqlite3.h" Define="HAVE_SQLITE3_H" />
    <Configuration Name="Debug">
      <Platform Name="Win32">
        <Include Name="vld.h" Optional="true" Define="HAVE_VLD_H" />
        <Library Name="vld.lib" Optional="true" />
      </Platform>
    </Configuration>
  </Project>
  
  <Project Name="OOSvrUser">
    <Configuration Name="Debug">
      <Platform Name="Win32">
        <Include Name="vld.h" Optional="true" Define="HAVE_VLD_H" />
        <Library Name="vld.lib" Optional="true" />
      </Platform>
    </Configuration>
  </Project>

  <Project Name="OOSvrReg">
    <Configuration Name="Debug">
      <Platform Name="Win32">
        <Include Name="vld.h" Optional="true" Define="HAVE_VLD_H" />
        <Library Name="vld.lib" Optional="true" />
      </Platform>
    </Configuration>
  </Project>

  <Project Name="OOReg">
    <Configuration Name="Debug">
      <Platform Name="Win32">
        <Include Name="vld.h" Optional="true" Define="HAVE_VLD_H" />
        <Library Name="vld.lib" Optional="true" />
      </Platform>
    </Configuration>
  </Project>

  <Project Name="OOGuidgen">
    <Configuration Name="Debug">
      <Platform Name="Win32">
        <Include Name="vld.h" Optional="true" Define="HAVE_VLD_H" />
        <Library Name="vld.lib" Optional="true" />
      </Platform>
    </Configuration>
  </Project>

  <Project Name="OOCOMLite">
    <Configuration Name="Debug">
      <Platform Name="Win32">
