  <Project Name="OORegEdit">
    <Include Name="wx/wxprec.h" />
    <Include Name="msvc/wx/setup.h" Append="msvc"/>
    <Configuration Name="Debug">
      <Library Name="wxmsw29ud_core.lib" />
      <Platform Name="Win32">
        <Include Name="vld.h" Optional="true" Define="HAVE_VLD_H" />
        <Library Name="vld.lib" Optional="true" />
      </Platform>
    </Configuration>
    <Configuration Name="Release">
      <Library Name="wxmsw29u_core.lib" />
    </Configuration>
  </Project>
  
  <Project Name="CoreTests">
    <Configuration Name="Debug">
      <Platform Name="Win32">
        <Include Name="vld.h" Optional="true" Define="HAVE_VLD_H" />
        <Library Name="vld.lib" Optional="true" />
