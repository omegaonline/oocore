<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="OOSvrLite" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug Win32">
				<Option output="OOSvrLite_vc8" prefix_auto="1" extension_auto="1" />
				<Option object_output="Debug Win32_Lite" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Option createStaticLib="1" />
				<Compiler>
					<Add option="-DHAVE_SQLITE3_AMALGAMATION" />
					<Add option="-Wall" />
					<Add option="-O0" />
					<Add directory="..\..\include" />
					<Add directory="D:\sqlite3" />
				</Compiler>
				<ResourceCompiler>
					<Add directory="..\..\include" />
					<Add directory="D:\sqlite3" />
				</ResourceCompiler>
				<Linker>
					<Add library="shlwapi" />
				</Linker>
				<ExtraCommands>
					<Add before="$(SolutionDir)msvc_conf.exe $(SolutionPath) OOSvrLite_vc8" />
				</ExtraCommands>
			</Target>
			<Target title="Release Win32">
				<Option output="OOSvrLite_vc8" prefix_auto="1" extension_auto="1" />
				<Option object_output="Release Win32_Lite" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Option createStaticLib="1" />
				<Compiler>
					<Add option="-DNDEBUG" />
					<Add option="-Wall" />
					<Add option="-O3" />
					<Add directory="..\..\include" />
				</Compiler>
				<ResourceCompiler>
					<Add directory="..\..\include" />
				</ResourceCompiler>
				<Linker>
					<Add library="shlwapi" />
				</Linker>
				<ExtraCommands>
					<Add before="$(SolutionDir)msvc_conf.exe $(SolutionPath) OOSvrLite_vc8" />
				</ExtraCommands>
			</Target>
		</Build>
		<Unit filename="..\Common\Server.h" />
		<Unit filename="..\Common\Version.h" />
		<Unit filename="Database.cpp" />
		<Unit filename="Database.h" />
		<Unit filename="IPSLite.cpp" />
		<Unit filename="IPSLite.h" />
		<Unit filename="OOServer_Lite.h" />
		<Unit filename="OOSvrLite.cpp" />
		<Unit filename="OOSvrLite_res.rc">
			<Option compilerVar="WINDRES" />
		</Unit>
		<Unit filename="RegistryCmn.h" />
		<Unit filename="RegistryHive.cpp" />
		<Unit filename="RegistryHive.h" />
		<Unit filename="SQLite3_comp.c">
			<Option compilerVar="CC" />
		</Unit>
		<Extensions>
			<code_completion />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
